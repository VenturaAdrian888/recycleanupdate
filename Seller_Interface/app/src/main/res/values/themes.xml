<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RecyclEarn" parent="Theme.AppCompat.DayNight.NoActionBar" >
        <item name="android:windowTranslucentStatus">true</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">#439dbb</item>
        <item name="colorPrimaryVariant">#b2ddd9</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="lollipop">#dce7d6</item>
        <!-- Customize your theme here. -->
    </style>
    
    <style name="navigationTheme">
        <item name="colorPrimary">@color/white</item>
        <item name="colorControlHighlight">@color/teal_700</item>
    </style>

</resources>